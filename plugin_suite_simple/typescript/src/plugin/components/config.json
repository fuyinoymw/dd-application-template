{
  "name": "请假套件-simple",
  "description": "请假套件demo版，看完就学会了套件开发",
  "icon": "",
  "bizType": "com.example.leave",
  "templateType": "simple",
  "props": { "hiddenReason": false },
  "children": [
    {
      "componentName": "SelectField",
      "bizAlias": "leaveType",
      "props": {
        "placeholder": "请选择",
        "label": "请假类型",
        "required": true,
        "options": [
          { "key": "option_1", "value": "病假" },
          { "key": "option_2", "value": "事假" }
        ]
      }
    },
    {
      "componentName": "DateField",
      "bizAlias": "leaveStartTime",
      "props": {
        "placeholder": "请选择",
        "label": "开始时间",
        "required": true,
        "format": "yyyy-MM-dd",
        "unit": "天"
      }
    },
    {
      "componentName": "DateField",
      "bizAlias": "leaveEndTime",
      "props": {
        "placeholder": "请选择",
        "label": "结束时间",
        "required": true,
        "format": "yyyy-MM-dd",
        "unit": "天"
      }
    }
  ]
}
